; Valve names
; The valves are grouped in classes as defined
; in this struct (saxed from a_base.cpp):
; 
;static struct { const char *Type; int Class; } actTypes[] = {
;// The last prefix character is "_", underscore, enables group structures
;// The prefix determines which class is applied, for instance;
;// reading an actuator preceeded by "FC_" envokes the  "FC_ACTUATOR" class.
;  {"GA_", CLASS_BASIC},  // Generic type prefix is GA
;  {"FC_", CLASS_FC},     // Fail close type prefix is FC
;  {"DH_", CLASS_DH},     // Downhole valves prefix is DH
;  {"CH_", CLASS_CHOKE},  // Unbalanced choke actuator prefix is CH
;  {"BCH_", CLASS_BCHOKE},// Balanced choke actuator prefix is BCH
;  {"WO_", CLASS_WO},      // Workover unlock cylinders prefix is WO
;  {"HM_", CLASS_HMOTOR},  // A standard hydraulic motor, prefix HM
;  {"BDH_", CLASS_BDH},    // Camco downhole valve, prefix BDH
;  {"MA_", CLASS_MA},      // A manifold actuator, described by piston and stem diameters
;  {"DW_", CLASS_DW},
;  {"AA_", CLASS_AA},      //A manifold actuator, described by piston and stem areas, open and close lines 
;  {"M3_", CLASS_M3},      // The FMC M3000 actuators
;  {"AC_", CLASS_AC},      //A FC manifold actuator, described by piston and stem areas, open line only 
;  {"BSGV_", CLASS_BSGV},    // Balanced Stem Gate Valve w M3000 actuator, spiral spring energized
;  {"BADH_", Class_BakerDh}, // The balanced Baker DH valve (with a  N2 charged compensating chamber)
;  {"FAM_", CLASS_FAM},      // Cameron/Willis Fast Actuating choke stroke Module
;  {"CW_", CLASS_CW},        // Cameron/Willis unbalanced stepping choke, prefix CW
;  {"BEL_", CLASS_BEL},
;  {"WDS_", CLASS_WDS},	     // The Well Dynamics Sliding sleeve class
;  {"FAI_", CLASS_FAI},      //Fail-as-is gate valve
;  {"FO_", CLASS_FO},        //Fail-open M3000 gate valve
;  {"M175_", CLASS_M175},    //FC M175 valve
;  {"FCH_", CLASS_FMCCHOKE}, //FMC stepping choke
;  {"BVFC_", CLASS_BVFC},    //Ball valve, fail close
;  {"BVFO_", CLASS_BVFO},    //Ball valve, fail open
;  {"M6_", CLASS_M6},        //BEL FC gate valve w internal compensator, Shah Deniz
;  {"FCVG_", CLASS_FCVG}, //Vetco Gray FC XT actuator, Troll B
;  {"M7", CLASS_M7},      //Cameron 717 bar FC gate valve, Linnorm
;  {"M8", CLASS_M8},      //Cameron 380 bar FC gate valve, Edradour - Glenlive
; #if defined(CLASS_HIPPS)
;	{"HI_", CLASS_HIPPS}, //The Vega SIPPS valve
; #endif
;  {NULL,0}
; };

